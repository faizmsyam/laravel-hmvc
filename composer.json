{
    "name": "faizmsyam/laravel-hmvc",
    "description": "HMVC structure for Laravel",
    "type": "library",
    "keywords": [
        "modules",
        "laravel",
        "module",
        "hmvc"
    ],
    "authors": [
        {
            "name": "Faiz Muhammad Syam",
            "email": "faizmsyam@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/framework": "^10.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "HT\\Modules\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HT\\Modules\\Providers\\ModuleServiceProvider"
            ]
        }
    },
    "scripts": {
        "pcf": "vendor/bin/php-cs-fixer fix --verbose"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
